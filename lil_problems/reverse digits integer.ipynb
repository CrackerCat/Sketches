   ],
   "source": [
    "def rev(a):\n",
    "    if a<0:\n",
    "        print('-',end='')\n",
    "        a = -a\n",
    "    while a>0:\n",
    "        b = a % 10\n",
    "        print(b,end='')\n",
    "        a = (a-b)//10\n",
    "    print()\n",
    "    \n",
    "rev(123)\n",
    "rev(-123)"
   ]
  }
