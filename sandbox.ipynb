    "def comp(s1,s2):\n",
    "    if not s1:         return len(s2)\n",
    "    if not s2:         return len(s1)\n",
    "    if s1[0]==s2[0]:   return comp(s1[1:],s2[1:])\n",
    "    return 1+min(comp(s1[1:], s2[1:]), comp(s1,s2[1:]), comp(s1[1:],s2))\n",
    "\n",
    "comp('nogazwu','nagatu')"
   ]
  }
